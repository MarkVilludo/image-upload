{
    "name": "mark-villudo/image-upload",
    "description": "Laravel package API for image-upload",
    "keywords": [
        "laravel",
        "api",
        "image-upload"
    ],
    "authors": [
        {
            "name": "Mark Villudo"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "intervention/image": "^2.4"
    },
    "autoload": {
        "psr-0": {
            "MarkVilludo\\ImageUpload\\": "src/"
        },
        "files": [
            "src/helper.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "MarkVilludo\\ImageUpload\\ServiceProvider"
            ],
            "aliases": {
                "ImageUpload": "MarkVilludo\\ImageUpload\\Facade"
            }
        }
    },
    "minimum-stability": "dev"
}
